<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;de.hybris.trainingSaurav.facades.product.data.GenderData>"/>
		<property name="manufacturerDetails"  type="java.util.List&lt;de.hybris.trainingSaurav.facades.product.data.ManufacturerDetailsData>"/>
		<property name="manufacturerDetailsNamelist"  type="java.util.List&lt;String>"/>
	</bean>

	<bean class="de.hybris.trainingSaurav.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	<bean class="de.hybris.trainingSaurav.facades.product.data.ManufacturerDetailsData">
		<property name="id" type="Integer"/>
		<property name="name" type="String"/>
		<property name="city" type="String"/>
		<property name="country" type="de.hybris.platform.commercefacades.user.data.CountryData"/>
	</bean>

	<bean class="de.hybris.trainingSaurav.facades.product.data.Customer1Data">
		<property name="customerId" type="Integer"/>
		<property name="customerName" type="String"/>
		<property name="customerCreated" type="java.util.Date"/>
		<property name="newCustomer" type="Boolean"/>
	</bean>
	
	
	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData">
		<property name="isNewCustomer" type="Boolean"/>
		<property name="isNewBussinessUser" type="Boolean"/>
	</bean>


	<bean class="de.hybris.trainingSaurav.facades.product.data.CustomRegisterData">
		<property name="isNewBussinessUser" type="Boolean"/>
	</bean>

	<bean class="de.hybris.platform.commercefacades.user.data.RegisterData">
		<property name="isNewBussinessUser" type="Boolean"/>
	</bean>


	<bean class="de.hybris.platform.solrfacetsearch.config.IndexConfig">
		<property name="userPriceGroup" type="de.hybris.platform.europe1.enums.UserPriceGroup"/>
	</bean>


	<bean class="de.hybris.platform.solrfacetsearch.config.IndexConfig">
	<property name="userGroup" type="java.util.List&lt;de.hybris.platform.core.model.user.UserGroupModel>"/>
    </bean>


    <bean class="de.hybris.platform.commercefacades.user.data.AddressData">
        <property name="VATNumber" type="String"/>
    </bean>




</beans>
